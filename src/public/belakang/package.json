   "homepage": "https://bit.ly/35ciMLb",
   "main": "index.js",
   "scripts": {
      "css": "node-sass application/css/src/ace.scss dist/css/ace.css && postcss --config build/postcss.config.js --replace \"dist/css/ace.css\"",
      "css-themes": "node-sass application/css/src/ace-themes.scss dist/css/ace-themes.css && postcss --config build/postcss.config.js --replace \"dist/css/ace-themes.css\"",
      "css-main": "node-sass --output-style expanded --source-map true --source-map-contents true application/css/src/ace.scss dist/css/ace.css && node-sass --output-style expanded --source-map true --source-map-contents true application/css/src/ace-themes.scss dist/css/ace-themes.css",
      "css-font": "node-sass --output-style expanded application/css/src/ace-font.scss dist/css/ace-font.css",
      "css-rtl": "rm -r \"dist/css/rtl\" && mkdir \"dist/css/rtl\" && rtlcss \"dist/css/ace.css\" \"dist/css/rtl/ace.css\" && rtlcss \"dist/css/ace-themes.css\" \"dist/css/rtl/ace-themes.css\" && rtlcss \"node_modules/bootstrap/dist/css/bootstrap.css\" \"dist/css/rtl/bootstrap.css\"",
      "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/css/**/*.css\"",
      "css-all": "npm-run-all css-main css-font css-prefix css-rtl",
      "css-min": "npm-run-all css-min-*",
      "css-min-main": "cleancss --format --source-map --source-map-inline-sources --output dist/css/ace.min.css dist/css/ace.css && cleancss --format --source-map --source-map-inline-sources --output dist/css/ace-font.min.css dist/css/ace-font.css && cleancss --format --source-map --source-map-inline-sources --output dist/css/ace-themes.min.css dist/css/ace-themes.css",
      "css-min-rtl": "cleancss --format --source-map --source-map-inline-sources --output dist/css/rtl/ace.min.css dist/css/rtl/ace.css && cleancss --format --source-map --source-map-inline-sources --output dist/css/rtl/ace-themes.min.css dist/css/rtl/ace-themes.css && cleancss --format --source-map --source-map-inline-sources --output dist/css/rtl/bootstrap.min.css dist/css/rtl/bootstrap.css",
      "css-docs": "node-sass ../ace-docs/assets/scss/style.scss ../ace-docs/assets/style.css",
      "js": "rollup --config build/rollup.config.js --sourcemap",
      "js-min": "npm-run-all js-min-*",
      "js-min-main": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/ace.js.map,includeSources,url=ace.min.js.map\" --output dist/js/ace.min.js dist/js/ace.js",
      "js-min-demo": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --output assets/js/demo.min.js assets/js/demo.js",
      "js-lint": "eslint application/js/src/*.js",
      "js-test": "karma start application/js/tests/karma.conf.js",
      "release-html": "cross-env HTML=true node index.js",
      "demo-html": "cross-env HTML=true BUILD=dist MIN=true node index.js",
      "release-package": "node build/archive.js",
      "demo-package": "cross-env PACKAGE=demo node build/archive.js",
      "prepare-all": "npm-run-all css-all css-min js js-min",
      "release-build": "npm-run-all release-html css-docs generate-docs release-package",
      "release": "npm-run-all prepare-all release-build && rm -r \"docs\" && rm -r \"html\"",
      "demo": "npm-run-all prepare-all demo-html demo-package && rm -r \"html\"",
      "generate-docs": "cross-env BUILD=true  OUTPUT_DIR=../ace/docs/html MODULES_DIR=../.. node ../ace-docs/index.js",
      "local": "node index.js"
   },
